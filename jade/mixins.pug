                span.link_text Дилерам
            li.menu_item
              +menu_link((active === 4 ? '_active' : ''), ref='trade.html')
                span.link_text Закупки
            li.menu_item
              +menu_link((active === 5 ? '_active' : ''), ref='about.html')
                span.link_text О компании
            li.menu_item
              +menu_link((active === 6 ? '_active' : ''), ref='vacancies.html')
                span.link_text Вакансии
            li.menu_item
              +menu_link((active === 7 ? '_active' : ''), ref='contacts.html')
                span.link_text Контакты

mixin hero_block(unreg)
  .hero_section
    .section_inner.clearfix
      .cart_holder
        .cart_wrapper
          .cart_corner_top
          .cart_corner_bottom
          .cart_block
            .cart_cell
              .cart_counter
                a.cart_link.fz_16(href='#') Корзина
                .cart_counter_val В корзине нет товаров

            .cart_cell
              ul.cart_menu
                - if (unreg) {
                  li._auth
                    a.cart_link(href='#') Вход/Регистрация
                    p
                      span.clr_orange *
                      span Зарегистрируйся и&nbsp;накапливай скидку!
                - } else {
                  li._lk
                    a.cart_link(href='#') Личный кабинет
                  li._lk
                    a.cart_link(href='#') Скидка клиента
                li._delivery
                  a.cart_link(href='#') Доставка и оплата
                - }

      .hero_block
        .heroSlider
          +loop_while(5)
            .hero_slide
              img.bsImg(src='i/photo_1.png')
              .hero_slide_content: .hero_info
                .slide_caption мы рады новым клиентам!
                p скидка на первую доставку - 10%
                .text_center
                  a.btn_v2.orange_btn.hero_btn(href='#')
                    span оформить заказ
          .hero_controls.heroControls

